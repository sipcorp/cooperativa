                        <i class="fontello-icon-plus-circle"></i>
                      </a>
                    </caption>
                    <thead>
                      <tr>
                        <th scope="col">ID</th>
                        <th scope="col">Name</th>
                        <th scope="col" class="action">Action</th>
                        <th scope="col" class="action">Delete</th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr ng-repeat="config in roles track by $index ">
                        <td>{{$index+1}}</td>
                        <td>{{config.name}}</td>
                        <td><i ng-repeat="action in config.module[0].action">{{action.name + " "}}</i>
                        </td>
                        <td class="text-center text-danger"><i class="icon-trash danger"
                            ng-click="modalVerify(config._id,config.name)"></i>
                        </td>
                      </tr>
                    </tbody>
                  </table>
                </div>
                <!-- // Table in well-nice -->
              </div>

