                  <div class="widget-header">
                    <h4><i class="fontello-icon-user"></i>{{role}}</h4>
                  </div>
                  <div class="widget-content">
                    <div class="widget-body">
                      <ul class="widget-list list-bordered thumb-xxlarge media-overflow">
                        <li class="media">
                          <div class="media-thumb media-left">
                            <a class="img-shadow" href="javascript:void(0);">
                              <img class="media-object thumb"
                                src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRTBlVaZW5VZUGFRsoobUv0Lh2J-zvZzLdMFwFFTEvleuTGRAdT"></a>
                          </div>
                          <div class="media-body">
                            <h4 class="media-heading">
                              <a href="javascript:void(0);">{{fullName}}</a>
                              <span class="label label-success pull-right">Active</span>
