                        <button data-parent="parent" class="btn module r" type="button" name="Read"
                          ng-click="check($event)">
                          <span>Read</span>
                        </button>
                        <button data-parent="parent" class="btn module m" type="button" name="Modify"
                          ng-click="check($event)">
                          <span>Modify</span>
                        </button>
                        <button data-parent="parent" class="btn module d" type="button" name="Delete"
                          ng-click="check($event)">
                          <span>Delete</span>
                        </button>
                      </div> -->
                      <div class="childrens">
                        <div ng-repeat="children in modelos.name">
                          <div class="btn-group mftop margin-bottom10">
                            <button id="{{children.name}}" class="btn sub" type="button"></button>
                          </div>
                          <span name="subModule">{{children.name}}</span>
                          <div class="btn-group margin-bottom10" style="margin-top:4px;margin-left:13px;">
                            <button data-parent="{{children.name}}" class="btn module r" type="button" name="Read"
                              ng-click="check($event)">
                              <span>Read</span>
                            </button>
                            <button data-parent="{{children.name}}" class="btn module m" type="button" name="Modify"
                              ng-click="check($event)">
                              <span>Modify</span>
                            </button>
                            <button data-parent="{{children.name}}" class="btn module d" type="button" name="Delete"
                              ng-click="check($event)">
                              <span>Delete</span>
                            </button>
                          </div>
                        </div>
                      </div>
                    </section>
                  </section>
                </div>
                <!-- // Table in well-nice -->
              </div>
              <!-- // Column -->
            </div>

            <div class="modal fade" id="ModalDelete" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
