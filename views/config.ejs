                    <legend>NUEVO ROLE
                      <a class="btn add" ng-click="add()">Salvar</a>
                    </legend>
                    <div class="alert alert-danger alert-dismissable span6" ng-show="showError"
                      style="position: absolute;top: 152px;left: 484px;">
                      <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                      {{error}}
                    </div>
                    <!-- // form item -->
                    <label for="formA05">NOMBRE:</label>
                    <input id="formA05" class="span6" type="text" placeholder="Nombre del role" ng-blur="check($event)">
                    <p class="help-block"><strong>Nota:</strong> Debe agregar los permisos para el
                      role
                    </p>
                  </section>
                  <section>
                    <legend>PERMISOS</legend>
                    <section ng-repeat="modelos in modules" style="border-bottom: 1px solid #e6e6e6;"
