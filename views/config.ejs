                              </li>
                              <li>
                                <abbr title="Phone"><i class="fontello-icon-mail-4 opaci75"></i>: </abbr>
                                <a href="javascript:void(0);">{{email}}</a>
                              </li>
                            </ul>
                          </div>
                        </li>
                      </ul>
                    </div>
                  </div>
                </div>
                <!-- // Widget -->
              </div>
            </div>
          </div>
          <!-- // tab 3 -->
         
          <!-- // tab 4 -->
          <div class="tab-pane" id="TabTop4" ng-controller="configController">
            <div class="row-fluid">
              <div class="span12">
                <table  class="table table-striped table-hover">
                  <caption>
                    Modulos Activos en su cuenta
                  </caption>
                  <thead>
                    <tr>
                      <th scope="col">ID <span class="column-sorter"></span></th>
