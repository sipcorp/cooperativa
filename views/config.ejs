                    <p class="d-flex justify-content-center">Desea Eliminar el Modulo <b>{{nameModule}}</b></p>
                  </div>
                  <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">NO</button>
                    <button type="button" class="btn btn-danger" id={{id}} ng-click="deleteRole(this.id)">SI</button>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <!-- // tab 2 -->
          <div class="tab-pane" id="TabTop2" ng-controller="userController">
            <div class="widget widget-box widget-table widget-list" ng-show="listUser">
