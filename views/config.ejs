                                  <input class="span11" type="text" name="fullName" value="" />
                                </div>
                              </li>
                              <!-- // form item -->
                              <li class="control-group">
                                <label for="accountLastName" class="control-label">Role
                                  <span class="required">*</span></label>
                                <div class="">
                                  <select id="roleSelect" ng-change="setRole(this)" ng-model="change">
                                    <option selected disabled>Seleccione un role</option>
                                    <option ng-repeat="role in roles" ng-disabled="role.name=='ADMIN'"
                                      value="{{role.name}}" ng-model="role">{{role.name}}</option>
                                  </select>
                                  <input type="text" id="roleSelected" name="role" value="" readonly
                                    style="opacity:0;" />
                                </div>
                              </li>
                              <!-- // form item -->
                              <li class="control-group">
                                <label for="accountSuffix" class="control-label">Cargo</label>
                                <div class="controls">
                                  <input id="accountSuffix" class="span11" type="text" name="cargo" />
                                </div>
                              </li>
                              <li class="control-group">
                                <label for="accountLastpassword" class="control-label">Contraseña
                                  <span class="required">*</span></label>
                                <div class="controls">
                                  <input id="password" class="span11" type="password" name="password" />
                                </div>
                              </li>
                              <!-- // form item -->
                              <li class="control-group">
                                <label for="accountGender" class="control-label">Genero</label>
                                <div class="controls">
                                  <input id="accountGender" type="hidden" name="gender" />
                                  <div id="accountGender" class="btn-group change" data-toggle="buttons-radio"
                                    data-target="accountGenderInput">
                                    <button type="button" class="btn" class-toggle="btn-green" value="male"
                                      ng-click="setGender('1')">
                                      <span>Hombre</span>
                                    </button>
                                    <button type="button" class="btn" class-toggle="btn-green" value="female"
                                      ng-click="setGender('2')">
                                      <span>Mujer</span>
                                    </button>
                                  </div>
                                </div>
                              </li>
                              <!-- // form item -->
                              <li class="control-group">
                                <label for="accountDob" class="control-label">Fecha de
                                  nacimiento <span class="required">*</span></label>
                                <div class="controls">
                                  <input id="accountDob" class="span11" type="date" name="date" value="" />
                                </div>
                              </li>
                              <!-- // form item -->
                              <li class="control-group">
                                <label for="accountEmail" class="control-label">Email
                                  <span class="required">*</span></label>
                                <div class="controls">
                                  <div class="input-append block">
                                    <input class="span11" type="email" name="email" value="" />

                                  </div>
                                </div>
                              </li>
                              <!-- // form item -->
                              <li class="control-group">
                                <label for="accountPhoneNumber" class="control-label">Telefono <span
                                    class="required">*</span></label>
                                <div class="controls controls-row">
                                  <input class="span11 maskPhone margin-right5" type="text" name="tel" value="" />

                                </div>
                              </li>
                              <!-- // form item -->
                              <li class="control-group">
                                <label for="accountFaxNumber" class="control-label">Movile</label>
                                <div class="controls controls-row">
                                  <input class="span11 maskPhone margin-right5" type="text" name="movile" value="" />

                                </div>
                              </li>
                              <!-- // form item -->
                            </ul>
                            <ul class="form-list label-left list-bordered dotted">
                              <li class="control-group">
                                <label for="accountAddressStreet" class="control-label">Direccion <span
                                    class="required">*</span></label>
                                <div class="controls controls-row">
                                  <textarea class="span11 margin-right5" type="text" name="direction"></textarea>
                                </div>
                              </li>
                            </ul>
                          </fieldset>
                          <!-- // fieldset Input -->
                        </div>
                      </div>
                    </form>
                  </div>
                </div>
              </div>
              <!-- // Widget -->
            </div>
            <div id="responsive" class="modal hide fade" tabindex="-1" data-width="375" data-height="250" style="background: #ffffff !important">
              <!--BOX FOR USER INFO OR EDIT INFO-->
              <div class="span4 grider" style="margin-left: 3px;margin-top: 6px; ">
